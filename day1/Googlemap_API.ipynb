    "import os\n",
    "from dotenv import load_dotenv\n",
    "import googlemaps\n",
    "\n",
    "load_dotenv()\n",
    "\n",
    "# get env variables\n",
    "googlemap_api_key = os.getenv('GOOGLEMAP_API_KEY')\n",
    "\n",
    "user_Key=googlemap_api_key\n",
    "\n",
    "gmaps=googlemaps.Client(key=user_Key)\n",
    "\n",
    "sch=input(\"Type address: \")\n",
    "ggmp=gmaps.geocode(sch, language='ko')\n",
    "ggmp"
   ]
